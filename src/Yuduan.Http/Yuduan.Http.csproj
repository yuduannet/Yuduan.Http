<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
     <TargetFrameworks>net452;netstandard2.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <Version>1.0.0.0</Version>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <RunPostBuildEvent>OnOutputUpdated</RunPostBuildEvent>
    <Authors>yuduan.net</Authors>
    <Company>Yuduan Network Technology Co., Ltd.</Company>
    <Product>Yuduan HttpClient Extension</Product>
    <Description>support Net Framework 4.5.2+ and net Standard 2.0</Description>
    <Copyright>Yuduan Network</Copyright>
  </PropertyGroup>

<PropertyGroup Condition=" '$(TargetFramework)' == 'net452' ">
    <DefineConstants></DefineConstants>
  </PropertyGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net452'">
      <Reference Include="System.Net.Http" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System.Web" />
  </ItemGroup>

</Project>
